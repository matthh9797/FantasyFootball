pay <- function(rate, time, overtime = 0, doubletime = 0, days = 1, weeks = 1, tax = 0.15, currency = "£", breaks = 0) {
perday <- ((rate*time) + (1.5*rate*overtime) + (2*rate * doubletime) - breaks)
